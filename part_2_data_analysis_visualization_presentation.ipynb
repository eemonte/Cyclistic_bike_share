   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "# drop these rides\n",
    "sub_biketrips = sub_biketrips[sub_biketrips['ended_at'] > sub_biketrips['started_at']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "# create a column \"ride_length\" by subtracting \"ended_at\" from \"started_at\"\n",
    "sub_biketrips.insert(4, \"ride_length\", sub_biketrips['ended_at'] - sub_biketrips['started_at'])"
   ]
